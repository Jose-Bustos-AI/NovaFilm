# OpenAI API Key for chatbot functionality
OPENAI_API_KEY=your_openai_api_key_here

# Kie.ai API Configuration
KIE_API_BASE=https://api.kie.ai/api/v1
KIE_API_KEY=your_kie_api_key_here

# Application Base URL (for callbacks)
APP_BASE_URL=https://your-replit-url.replit.dev

# Database Configuration (Neon PostgreSQL)
DATABASE_URL=postgresql://username:password@hostname:port/database

# JWT Secret for sessions (provided by Replit)
SESSION_SECRET=your_session_secret_here

# Environment
NODE_ENV=development

# Replit Configuration (automatically set by Replit)
REPL_ID=your_repl_id
REPLIT_DOMAINS=your-replit-url.replit.dev
ISSUER_URL=https://replit.com/oidc
